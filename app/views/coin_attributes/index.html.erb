<h1>Listing coin_attributes</h1>

<table>
  <tr>
    <th>Coin</th>
    <th>Coin attribute type id</th>
    <th>Coin attribute value</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @coin_attributes.each do |coin_attribute| %>
  <tr>
    <td><%= coin_attribute.coin_id %></td>
    <td><%= coin_attribute.coin_attribute_type_id %></td>
    <td><%= coin_attribute.coin_attribute_value %></td>
    <td><%= link_to 'Show', coin_attribute %></td>
    <td><%= link_to 'Edit', edit_coin_attribute_path(coin_attribute) %></td>
    <td><%= link_to 'Destroy', coin_attribute, :method => :delete, :data => { :confirm => 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

